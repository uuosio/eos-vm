TEST_CASE( "Testing wasm <memory_trap_test_0_wasm>", "[memory_trap_test_0_wasm_tests]" ) {
   auto code = backend_t::read_wasm( memory_trap_test_0_wasm ) );
   backend_t bkend( code );
   bkend.set_wasm_allocator( &wa );

   CHECK(!bkend.call_with_return(nullptr, "env", "store", static_cast<uint32_t>(4294967292), static_cast<uint32_t>(42), )));
   CHECK(to_i32(*bkend.call_with_return(nullptr, "env", "load", static_cast<uint32_t>(4294967292))) == static_cast<uint32_t>(42));
   CHECK_THROWS_AS(bkend(nullptr, "env", "store", static_cast<uint32_t>(4294967293), static_cast<uint32_t>(13), ), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "load", static_cast<uint32_t>(4294967293)), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "store", static_cast<uint32_t>(4294967294), static_cast<uint32_t>(13), ), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "load", static_cast<uint32_t>(4294967294)), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "store", static_cast<uint32_t>(4294967295), static_cast<uint32_t>(13), ), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "load", static_cast<uint32_t>(4294967295)), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "store", static_cast<uint32_t>(0), static_cast<uint32_t>(13), ), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "load", static_cast<uint32_t>(0)), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "store", static_cast<uint32_t>(2147483648), static_cast<uint32_t>(13), ), std::exception);
   CHECK_THROWS_AS(bkend(nullptr, "env", "load", static_cast<uint32_t>(2147483648)), std::exception);
   CHECK(to_i32(*bkend.call_with_return(nullptr, "env", "memory.grow", static_cast<uint32_t>(65537))) == static_cast<uint32_t>(4294967295));
}

