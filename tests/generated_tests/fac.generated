TEST_CASE( "Testing wasm <fac_test_0_wasm>", "[fac_test_0_wasm_tests]" ) {
   auto code = backend_t::read_wasm( fac_test_0_wasm ) );
   backend_t bkend( code );
   bkend.set_wasm_allocator( &wa );

   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-rec", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-iter", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-rec-named", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-iter-named", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-opt", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
}

