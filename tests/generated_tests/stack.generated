TEST_CASE( "Testing wasm <stack_test_0_wasm>", "[stack_test_0_wasm_tests]" ) {
   auto code = backend_t::read_wasm( stack_test_0_wasm ) );
   backend_t bkend( code );
   bkend.set_wasm_allocator( &wa );

   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-expr", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-stack", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
   CHECK(to_i64(*bkend.call_with_return(nullptr, "env", "fac-mixed", static_cast<uint64_t>(25))) == static_cast<uint64_t>(7034535277573963776));
}

